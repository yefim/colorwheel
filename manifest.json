{
  "manifest_version": 2,
  "name": "colorwheel",
  "version": "2.0.0",
  "description": "colorwheel previews colors and easily converts between hex and rgb.",
  "icons": {
    "19": "images/colorwheel16.png",
    "38": "images/colorwheel48.png"
  },
  "browser_action": {
    "default_icon": "images/colorwheel48.png",
    "default_title": "colorwheel",
    "default_popup": "colorwheel.html"
  },
  "background": {
    "page": "empty.html"
  },
  "permissions": ["storage"]
}
